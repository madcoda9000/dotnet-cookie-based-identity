{
    "ConnectionStrings": {
      "SqlServer": "",
      "MySql": "server=YORD_DB_SERVER; database=YOUR_DB_NAME; user=rYOUR_DB_USER; password=YOUR_DB_PASSWD"
    },
    "AppSettings": {
      "DataBaseType": "MySql",
      "SessionCookieExpiration" :  "7",
      "DataBaseName": "YOUR_DB_NAME"
    },
    "Logging": {
      "LogLevel": {
        "Default": "Warning",
        "Microsoft.AspNetCore": "Information",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },  
  "Serilog": {
    "Using": [
      "Serilog.Sinks.MariaDB"
    ],
    "MinimumLevel": "Information",
    "Override": {
      "Microsoft": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore": "Information"
    },
    "WriteTo": [
      {
        "Name": "MariaDB",
        "Args": {
          "connectionString": "server=YORD_DB_SERVER; database=YOUR_DB_NAME; user=rYOUR_DB_USER; password=YOUR_DB_PASSWD",
          "autoCreateTable": true,
          "tableName": "AppLogs",
          "restrictedToMinimumLevel": "Information",
          "batchPostingLimit": 1000,
          "period": "0.00:00:30",
          "options": {
            "PropertiesToColumnsMapping": {
              "Exception": "Exception",
              "Level": "Level",
              "Message": "Message",
              "MessageTemplate": "MessageTemplate",
              "Properties": "Properties",
              "Timestamp": "Timestamp"
            },
            "TimestampInUtc": false,
            "ExcludePropertiesWithDedicatedColumn": true,
            "EnumsAsInts": true,
            "LogRecordsCleanupFrequency": "01.00:00:00",
            "LogRecordsExpiration": "30.01:00:00"
          }
        }
      }
    ]
  },
    "AllowedHosts": "*"
  }